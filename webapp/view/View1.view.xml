<mvc:View controllerName="fragmentproject.fragmentdialogproject.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
    <Page id="page" title="{i18n>title}">
        <content>
            <VBox alignContent="Center" alignItems="Center">
                <f:SimpleForm alignContent="Center" alignItems="Center">
                    <f:content>
                        <l:VerticalLayout width="100%">
                            <Label text="Region Id" labelFor="regId" />
                            <Input id="regnId" showSuggestion="true" showValueHelp="true" textFormatMode="KeyValue" valueHelpRequest="onLoadDialog" />
                        </l:VerticalLayout>
                    </f:content>
                </f:SimpleForm>
            </VBox>

            <!-- <Button text="Open the dailog" press="onOpenDialog" class="sapUiSmallMarginEnd" /> -->
        </content>
    </Page>
</mvc:View>
